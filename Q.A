#include<stdio.h>
#include<stdlib.h>

int main()
{
    float a,b,c,D,e,f,j;

    printf("Digite 3 notas:\n");
    scanf("%f %f %f", &a, &b, &c);

    printf("Digite seus respectivos pesos:\n");
    scanf("%f %f %f", &e, &f, &j );

    D =( (a*e) + (b*f) + (c*j) ) / (e+f+j) ;
    
    printf("Sua media eh: %f", D);
}
